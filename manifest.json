{
    "name": "FlashNotes",
    "version": "1.01",
    "description": "CI103-69-94 Project",
    "manifest_version": 2,

    "permissions": [
    "activeTab",
    "clipboardWrite",
    "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
      "default_icon": "icon.png"
    }
}
